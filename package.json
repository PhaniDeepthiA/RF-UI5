{
  "name": "inboundlabel",
  "version": "0.0.1",
  "description": "An SAP Fiori application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "dependencies": {
    "cds": "^0.2.0"
  },
  "devDependencies": {
    "@sap/ux-ui5-tooling": "1",
    "@ui5/cli": "^4.0.33",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5",
    "ui5-task-zipper": "^3.4.x",
    "@sap-ux/ui5-middleware-fe-mockserver": "2"
  },
  "scripts": {
    "start": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"index.html\"",
    "start-noflp": "fiori run --open \"index.html\"",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "deploy": "fiori cfDeploy",
    "deploy-config": "fiori add deploy-config",
    "int-test": "fiori run --open \"test/integration/opaTests.qunit.html\"",
    "unit-test": "fiori run --open \"test/unit/unitTests.qunit.html\"",
    "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateCachebusterInfo",
    "build:mta": "rimraf resources mta_archives && mbt build",
    "undeploy": "cf undeploy inboundlabel --delete-services --delete-service-keys --delete-service-brokers",
    "start-mock": "fiori run --config ./ui5-mock.yaml --open \"index.html?sap-ui-xx-viewCache=false\""
  },
  "sapuxLayer": "CUSTOMER_BASE"
}
